            </button>
        </div>*@
    </div>
    <div class="row">
        <div class="col-lg-6 " id="div_received">

            <div class="ibox float-e-margins dim" style="box-shadow : 5px 5px 5px 5px #d3d3d3;">
                <div class="ibox-title " style="background-color:#507CD1;border-radius:5px 5px 0px 0px;color:white">
                    <h5> <i class="fa fa-mail-reply"></i> TO BE RECEIVED </h5>
                    <div class="ibox-tools">
                        <span class="badge badge-danger ">{{total_count_received}}</span>
                    </div>
                </div>
                <div class="ibox-content" style="padding-top:5px !important">
                    <div class="col-sm-5 no-padding">
                        <div class="form-group row">
                            <div class="col-lg-3">
                                <small class="pull-left font-bold" style="margin-top:9px;"><b>Show</b></small>
                            </div>
                            <div class="col-lg-6">
                                <select class="form-control field-select-sm" ng-model="rowLen" ng-change="setNumOfRow(rowLen,'torecieve')">
                                    <option value="5">5</option>
                                    <option value="10" selected>10</option>
                                    <option value="15">15</option>
                                    <option value="20">20</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-7 no-padding">
                        <input type="search" class="form-control" ng-model="search_box_receive" ng-keyup="search_in_list(search_box_receive,'torecieve')" placeholder="Search ....">
                    </div>
                    <div class="col-12 table-resposive">
                        <table id="torecieve" class="table table-striped table-bordered table-hover table-responsive">
                            <thead>
                                <tr>
                                    <th style="width:20px !important "></th>
                                    <th style="width:100px !important">DOC. CTRL. #</th>
                                    <th style="width:220px !important">DESCRIPTION</th>
                                    <th>ACTIONS</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>

                </div>
            </div>
        </div>

        <div class="col-lg-6" id="div_release">
            <div class="ibox float-e-margins" style="box-shadow : 5px 5px 5px 5px #d3d3d3">
                <div class="ibox-title" style="background-color:#18a689;border-radius:5px 5px 0px 0px;color:white">
                    <h5> <i class="fa fa-mail-forward"></i> TO BE RELEASED / RETURNED</h5>
                    <div class="ibox-tools">
                        <span class="badge badge-warning">{{total_count_released}}</span>

                    </div>
                </div>
                <div class="ibox-content ">
                    <div class="col-sm-5 no-padding">
                        <div class="form-group row">
                            <div class="col-lg-3">
                                <small class="pull-left font-bold" style="margin-top:9px;"><b>Show</b></small>
                            </div>
                            <div class="col-lg-6">
                                <select class="form-control field-select-sm" ng-model="rowLen" ng-change="setNumOfRow(rowLen,'torelease')">
                                    <option value="5">5</option>
                                    <option value="10" selected>10</option>
                                    <option value="15">15</option>
                                    <option value="20">20</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-7 no-padding">
                        <input type="search" class="form-control" ng-model="search_box_release" ng-keyup="search_in_list(search_box_release,'torelease')" placeholder="Search ....">
                    </div>

                    <div class="col-12 table-resposive">
                        <table id="torelease" class="table table-striped table-bordered table-hover table-responsive" style="width:100% !important">
                            <thead>
                                <tr>
                                    <th style="background-color:#18a689 !important;width:20px !important "></th>
                                    <th style="background-color:#18a689 !important;width:100px !important">DOC. CTRL. #</th>
                                    <th style="background-color:#18a689 !important;width:220px !important">DESCRIPTION</th>
                                    <th style="background-color:#18a689 !important;">ACTIONS</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" id="id_document_info" tabindex="-1" role="dialog" aria-labelledby="modalLabelSmall" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content  modal-content-add-edit">
                <div class="modal-header bg-primary text-center">
                    
                    @*<h2 class="modal-title"><b> <i class="fa fa-info-circle"></i> DOCUMENT INFORMATION</b></h2>*@
                    <div class="form-group">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                        <h2 class="col-lg-12 text-center text-white" style="margin-top:0px;margin-bottom:2px;line-height: 0.7;">
                            <b>SEARCH DOCUMENT CONTROL NUMBER</b>
                            <br />
                            <small class="text-info text-center" style="font-size:12px"> <b> <i class="fa fa-info-circle"></i> Scan the document or input the document control number   </b></small>
                        </h2>
                        <div class="col-lg-3">
                        </div>
                        <div class="col-lg-6 text-center">
                            <span style="position:absolute;float:right;right:5%;padding-top:2px;color:dimgrey;font-size:35px"> <i class="fa fa-qrcode"></i> <i class="fa fa-barcode"></i></span>
                            <input type="text" class="form-control text-center" autofocus style="height:50px;font-size:29px;color:darkslategray;letter-spacing: 0.154em;padding-right:80px" id="doc_ctrl_nbr" ng-model="doc_ctrl_nbr" ng-keyup="scan_doc_ctrl_nbr(doc_ctrl_nbr)">
                        </div>
                        <div class="col-lg-3">

                        </div>
                        <small id="barcode_notfound" class="col-lg-12 text-info text-center hidden" style="font-size:12px;height:15px;color:red"> <b> <i class="fa fa-info-circle"></i> Barcode not found!   </b></small>
                    </div>

                </div>
                <div class="modal-body with-background" style="padding:10px;">
                    <div class="row">
                        <div class="col-lg-12">

                            <div class="form-group">
                                <div class="row">
                                    <div class="col-lg-12">
                                        <div class="panel-body " style="padding-bottom:0px">
                                            <div class="panel-group" id="accordion">
                                                <div class="panel panel-success">
                                                    <div class="panel-heading ">
                                                        <h4 class="panel-title">
                                                            <a data-toggle="collapse" id="a_collapse" data-parent="#accordion" href="#collapseOne"> <i class="fa fa-eye"></i> Document Information </a>
                                                        </h4>

                                                        @*<h2 class="col-lg-3 control-label page-title" style="padding-bottom:10px;font-size:21px">Document Ctrl. Nbr. :</h2>
                                                            <div class="col-lg-9">
                                                                <span style="position:absolute;float:right;right:10%;padding-top:2px;color:dimgrey;font-size:35px"> <i class="fa fa-barcode"></i></i></span>
                                                                <input type="text" class="form-control" style="height:50px;font-size:25px">
                                                                <small class="text-danger text-right" > <b> Scan the documents or Input the Document Control number *</b></small>
                                                            </div>*@

                                                    </div>
                                                    <div id="collapseOne" class="panel-collapse collapse in">
                                                        <div class="panel-body with-background modal-content-add-edit">
                                                            <form ng-model="di">
                                                                <div class="form-group">

                                                                    <span class="col-lg-3 font-bold">Date {{rel_rec_ret}}:</span>

                                                                    <div class="col-lg-9" style="margin-bottom:3px">
                                                                        <div class='input-group date' id='datetimepicker1' style="width:50%" disabled>
                                                                            <input type='text' id="dttm" ng-model="di.dttm" class="form-control text-center" disabled />
                                                                            <span class="input-group-addon" ng-click="editDate()" disabled>
                                                                                <span class="fa fa-calendar" disabled></span>
                                                                            </span>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="form-group">
                                                                    <span class="col-lg-3 font-bold ">Description : </span>
                                                                    <div class="col-lg-9" style="margin-bottom:3px">
                                                                        <input type="text" class="form-control" id="payroll_registry_descr" ng-model="di.payroll_registry_descr" disabled>
                                                                    </div>
                                                                </div>
                                                                <div class="form-group">
                                                                    <span class="col-lg-3  font-bold">Remarks : </span>
                                                                    <div class="col-lg-9" style="margin-bottom:3px">
                                                                        <textarea class="form-control" id="remarks" ng-model="di.remarks" ng-keyup="OnKeyup('remarks',di.remarks)"></textarea>
                                                                    </div>
                                                                </div>
                                                                <div class="form-group" ng-show="doc_nbr_show && doc_type1_show">
                                                                    <span class="col-lg-3 font-bold ">CAFOA Control Nbr : </span>
                                                                    <div class="col-lg-9" style="margin-bottom:3px">
                                                                        <input type="text" class="form-control" ng-disabled="(dis_in_review || dis_cfo_nbr)" style="width:50%" id="doc_cafoa_nbr" ng-model="di.doc_cafoa_nbr" ng-keyup="nbr_onkeyup('doc_cafoa_nbr',di.doc_nbr)" />
                                                                    </div>
                                                                </div>
                                                                <div class="form-group" ng-show="doc_nbr_show">
                                                                    <span class="col-lg-3 font-bold ">{{doc_nbr_lbl}} : </span>
                                                                    <div class="col-lg-9" style="margin-bottom:3px">
                                                                        <input type="text" class="form-control" style="width:50%" id="doc_nbr" ng-disabled="(dis_in_review || dis_v_nbr)" ng-model="di.doc_nbr" ng-keyup="nbr_onkeyup('doc_nbr',di.doc_nbr)" />
                                                                    </div>
                                                                </div>
                                                                @*<div class="form-group" ng-show="doc_funcode_show">*@
                                                                <div class="form-group" ng-show="doc_nbr_show && doc_type1_show">
                                                                    <span class="col-lg-3 font-bold ">Fund Code : </span>

                                                                    @*<div class="col-lg-9" style="margin-bottom:3px" ng-show="doc_funcode_show">*@
                                                                    <div class="col-lg-9" style="margin-bottom:3px" ng-show="doc_nbr_show">
                                                                        <select class="form-control" id="doc_fund_subcode" ng-disabled="dis_in_review" ng-model="di.doc_fund_subcode">
                                                                            <option value=""> -- Select here --</option>
                                                                            <option ng-repeat="l in docfundcode" value="{{l.cafund_subcode}}">{{l.cafund_subdescr}}</option>
                                                                        </select>
                                                                    </div>
                                                                </div>

                                                                <div class="form-group">
                                                                    <span class="col-lg-3  font-bold" ng-show="show_release_btn">Release to : </span>
                                                                    <div class="col-lg-3" style="margin-bottom:3px" ng-show="show_release_btn">
                                                                        <select class="form-control" id="dd_ToRelease_route" ng-model="di.dd_ToRelease_route" ng-click="ToReleaseRouteSelect(di.dd_ToRelease_route)">
                                                                            <option value="">-- Select here --</option>
                                                                            <option ng-repeat="l in to_release_route track by $index" ngx-index="{{$index}}" value="{{l.rte_code}}">{{l.route_descr}}</option>
                                                                        </select>
                                                                    </div>
                                                                    <div class="col-lg-3 pull-right" style="margin-bottom:5px" ng-show="show_return_btn">
                                                                        <select class="form-control" id="dd_ToReturn_route" ng-model="di.dd_ToReturn_route" ng-change="ToReturnRouteSelect(di.dd_ToReturn_route)">
                                                                            <option value="">-- Select here --</option>
                                                                            <option ng-repeat="l in to_return_route track by $index" ngx-index="{{$index}}" value="{{l.rte_code}}">{{l.route_descr}}</option>
                                                                        </select>
                                                                    </div>
                                                                    <span class="col-lg-2 font-bold pull-right" style="margin-top:5px;" ng-show="show_return_btn">Return to : </span>
                                                                </div>
                                                                <small id="requirefields" class="col-lg-12 text-info text-center hidden" style="font-size:12px;height:15px;color:red">  <i class="fa fa-info-circle"></i>Required fields empty! </small>
                                                                <div class="form-group">
                                                                    <div class="col-lg-12 text-right" style="margin-top:20px;padding-right:5px">
                                                                        <button class="btn btn-primary dim" id="rlsd" ng-show="allow_release" ng-click="ReleaseRoute(di)"><i id="rlsd" class="btn_rlsd fa fa-forward"></i> Release</button>
                                                                        <button class="btn btn-success dim" id="rcvd" ng-show="allow_receive" ng-click="ReceiveRoute(di)"><i id="rcvd" class="btn_rcvd fa fa-backward"></i> Receive</button>
                                                                        <button class="btn btn-danger  dim" id="retd" ng-show="allow_return" ng-click="ReturnRoute(di)"> <i id="retd" class="btn_retd fa fa-times"></i> Return</button>
                                                                    </div>
                                                                </div>
                                                            </form>

                                                        </div>
                                                    </div>
                                                </div>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" id="id_document_history" tabindex="-1" role="dialog" aria-labelledby="modalLabelSmall" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content  modal-content-add-edit">
                <div class="modal-header bg-primary  text-center">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    @*<h2 class="modal-title"><b> <i class="fa fa-info-circle"></i> DOCUMENT INFORMATION</b></h2>*@
                    <div class="form-group" style="margin-bottom:40px;">
                        <h2 class="col-lg-12 text-center text-white" style="margin-top:0px;margin-bottom:2px;line-height: 0.7;">
                            <b>Document Tracking History</b>
                        </h2>
                    </div>

                </div>
                <div class="modal-body with-background" style="padding:10px;">
                    <div class="row">
                        <div class="col-lg-12">

                            <div class="form-group">
                                <div class="row">
                                    <div class="col-lg-12">
                                        <div class="panel-body " style="padding-bottom:0px">
                                            <div class="panel-group" id="accordion">
                                                <div class="panel panel-warning">
                                                    <div class="panel-heading">
                                                        <h4 class="panel-title">
                                                            <a data-toggle="collapse" data-parent="#accordion" href="#collapseThree"><i class="fa fa-book"></i> Search Documents </a>
                                                        </h4>
                                                    </div>
                                                    <div id="collapseThree" class="panel-collapse collapse in">
                                                        <div class="panel-body" style="background-image:linear-gradient(white,#ffe5c8)">
                                                            <div class="row">
                                                                <span class="col-lg-3  font-bold">Document Ctrl Nbr :</span>
                                                                <div class="col-lg-3" style="margin-bottom:3px">
                                                                    <input type="text" class="form-control" id="txtb_search_doc_ctrl_nbr" ng-model="txtb_search_doc_ctrl_nbr">
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <span class="col-lg-3  font-bold">Payroll Registry Descr :</span>
                                                                <div class="col-lg-9" style="margin-bottom:3px">
                                                                    <input type="text" class="form-control" id="txtb_search_doc_descr" ng-model="txtb_search_doc_descr">
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <span class="col-lg-3  font-bold" ng-show="!hide_docs_type">Payroll Template :</span>
                                                                <div class="col-lg-9" style="margin-bottom:3px" ng-show="!hide_docs_type">

                                                                    <input type="text" class="form-control" id="txtb_search_doc_descr" ng-model="ddl_payrolltemplate_code">
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class="col-lg-12 text-right" style="padding-right:5px">
                                                                    <button class="btn btn-warning dim" ng-click="btn_search_document()">   <i class="fa fa-search"></i>  Search </button>
                                                                </div>
                                                            </div>

                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <div class="modal fade" id="id_modal_search_docs" tabindex="-1" role="dialog" aria-labelledby="modalLabelSmall" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content  modal-content-add-edit">
                <div class="modal-header bg-primary  text-center">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    <div class="form-group" style="margin-bottom:40px;">
                        <h2 class="col-lg-12 text-center text-white" style="margin-top:0px;margin-bottom:10px;line-height: 0.7;">
                            <b>SEARCH DOCUMENTS</b>
                            <br />
                            <small class="text-info text-center" style="font-size:10px;"> <b> <i class="fa fa-info-circle"></i> View History, Search and Print Documents  </b></small>
                        </h2>

                    </div>

                </div>
                <div class="modal-body with-background" style="padding:10px;">
                    <div class="row">
                        <div class="col-lg-12">

                            <div class="form-group">
                                <div class="row">
                                    <div class="col-lg-12">
                                        <div class="panel-body " style="padding-bottom:0px">
                                            <div class="panel-group" id="accordion">
                                                <div class="panel panel-success">
                                                    <div class="panel-heading ">
                                                        <h4 class="panel-title">
                                                            <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne1"> <i class="fa fa-eye"></i> View Document History </a>
                                                        </h4>
                                                    </div>
                                                    <div id="collapseOne1" class="panel-collapse collapse">
                                                        <div class="panel-body with-background modal-content-add-edit">

                                                            <div class="form-group" style="padding-right: 50px; padding-left: 50px;">
                                                                <span class="col-lg-4 font-bold " style="padding-top: 10px; font-size: 15px; font-weight: bold;">Document Control Number : </span>
                                                                <div class="col-lg-8" style="margin-bottom:3px">
                                                                    <span style="position:absolute;float:right;right:30%;padding-top:2px;color:dimgrey;font-size:35px"> <i class="fa fa-qrcode"></i> <i class="fa fa-barcode"></i></span>
                                                                    <input type="text" id="doc_ctrl_nbr_print_history" ng-model="doc_ctrl_nbr_print_history" class="form-control text-center" style="border-radius: 5px 0px 0px 5px;width: 75%; height: 50px; font-size: 25px; float: left;letter-spacing: 0.154em;padding-right:20%">
                                                                    <button class="btn btn-primary" type="button" style="border-radius: 0px 5px 5px 0px; width: 25%; height: 50px; font-weight: bolder; float: right;" ng-click="btn_print_dochistory()" id="btn_print_dochistory">Print History </button>
                                                                </div>

                                                            </div>

                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="panel panel-primary">
                                                    <div class="panel-heading">
                                                        <h4 class="panel-title">
                                                            <a data-toggle="collapse" data-parent="#accordion" href="#collapseTwo"><i class="fa fa-search"></i> Print Received, Released, or Returned Documents</a>
                                                        </h4>
                                                    </div>
                                                    <div id="collapseTwo" class="panel-collapse collapse">
                                                        <div class="panel-body" style="background-image:linear-gradient(white,#85e398)">
                                                            <form ng-model="ds">
                                                                <div class="form-group row">
                                                                    <div class="col-lg-4">
                                                                        <span class="font-bold pull-left">Document Status List:</span>

                                                                        <select class="form-control" ng-model="ds.doc_status">
                                                                            @*<option value="">--- Select Here ---</option>*@
                                                                            <option value="V" selected>Received Documents</option>
                                                                            <option value="L">Released Documents</option>
                                                                            <option value="T">Returned Documents</option>
