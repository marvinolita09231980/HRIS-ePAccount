@model HRIS_ePAccount.Models.User_Menu
@{
    ViewBag.Title = "Index";
}
<style type="text/css">
    .field {
        border-radius: 3px;
        height: 35px;
        font-size: 18px;
    }

    .field-arrow {
        font-size: 26px;
    }

    .require-field {
        border: 1px solid red !important;
    }

    .currency {
        text-align: right;
    }

    .modal-background {
        background-image: url('../ResourcesImages/add_background.png'),linear-gradient(white, lightblue);
        background-position: center;
        background-repeat: no-repeat;
        background-size: cover;
    }

    .fld-lbl {
        font-size: 12px !important;
        margin: 2px;
        padding: 3px;
    }

    .fld-val {
        margin: 2px;
        padding: 3px;
    }

    .hdr-lbl {
        font-size: 14px !important;
        margin: 0;
        padding: 3px;
    }

    .hdr-val {
        height: 35px;
        margin: 2px;
        padding: 3px;
        font-size: 14px;
    }

    table tr td {
        padding: 0;
        margin: 0;
    }

    .margin-top {
        margin-top: 8px;
    }

    .font-bold {
        font-weight: bold !important;
    }
</style>

<script src="~/Angular/ng-cRemitLedgerHDMF.js"></script>
