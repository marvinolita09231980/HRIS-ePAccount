                                        <div style="width:42.3%; float:right;">
                                            <input class="form-control currency" ng-model="fd.txtb_uploaded_gs" disabled />
                                        </div>

                                    </div>
                                </div>*@
                        }

                    </div>
                    <div class="modal-footer" style="padding: 10px 30px 10px 30px !important">
                        <div class="row">

                            <div class="col-sm-8"></div>
                            <div class="col-sm-4 pull-right no-padding">
                                <button type="button" class="btn btn-danger" data-dismiss="modal"><i class="fa fa-close"></i> Close</button>
                                <button ng-show="!isEdit" ng-click="SaveHdmfDetails(fd)" type="button" class="btn btn-success"><i class="fa fa-save"></i> Save</button>
                                @if (Model.remittancetype_code == "02")
